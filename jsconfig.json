{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "src",
    "paths": {
      "app/*": ["app/*"],
      "components/*": ["app/components/*"],
      "pages/*": ["app/pages/*"],
      "config/*": ["configs/*"],
      "shared/*": ["shared/*"],
      "utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}
